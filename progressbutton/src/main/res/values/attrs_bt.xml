<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressButton">
        <!--format:格式-->
        <!--reference：参考某一资源ID；color：颜色-->
        <attr name="normalBackground" format="reference|color"/>
        <attr name="progressBackground" format="reference|color"/>
        <attr name="completeBackground" format="reference|color"/>
        <attr name="textColor" format="color"/>
        <attr name="nomalText" format="string"/>
        <attr name="progressText" format="string"/>
        <attr name="completeText" format="string"/>
        <attr name="errorText" format="string"/>
    </declare-styleable>
</resources>